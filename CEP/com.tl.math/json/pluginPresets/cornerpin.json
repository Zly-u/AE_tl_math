"gl33_vert_sh": "#version 330 \\nin vec4 Position;\\nin vec2 UVs;\\nout vec4 out_pos;\\nout vec2 out_uvs;\\nuniform vec2 resolution;\\nuniform vec3 point_1;\\nuniform vec3 point_2;\\nuniform vec3 point_3;\\nuniform vec3 point_4;\\nuniform mat4 ModelviewProjection;\\n\\nvec2 normalize_pt (vec3 pt, vec2 resolution){\\n    vec2 center = resolution.xy/2;\\n    return vec2(-(center-pt.xy)/center);\\n}\\nvoid main(void)\\n{\\nout_pos = ModelviewProjection * Position;\\n\\n gl_Position = out_pos;\\n if (gl_VertexID == 3){\\n        gl_Position.xy = normalize_pt(point_2.xyz, resolution);\\n    }\\n if (gl_VertexID ==2){\\n        gl_Position.xy= normalize_pt(point_1.xyz, resolution);\\n    }\\n if (gl_VertexID ==0){\\n        gl_Position.xy = normalize_pt(point_4.xyz, resolution);\\n    }\\n if (gl_VertexID ==1){\\n        gl_Position.xy = normalize_pt(point_3.xyz, resolution);\\n    }\\n \\nout_uvs = UVs;\\n}"

