"gl33_frag_error": "compiled successfully",


"gl33_frag_sh": "#version 330 // glsls version for opengl 3.3\\nin vec4 out_pos;\\nin vec2 out_uvs;\\nuniform float timeS;\\nout vec4 fragColorOut;\\n\\nuniform float Speed;\\nuniform float Offset;\\nuniform float Thickness;\\nuniform float zoom;\\nuniform  float itterations;\\nuniform  float details;\\nuniform  float x;\\nuniform  float y;\\nuniform  float z;\\nuniform vec2 resolution;\\nuniform vec3 direction;  \\nuniform vec2 center;\\nuniform vec3 off;\\nuniform vec3 tint;\\n\\n\\nint  Itterations = int(itterations);\\nint res = int(details);\\n\\n\\n\\nfloat p1 = normalize(off.x);\\nfloat p2 = normalize(off.y);\\n\\n\\nfloat time = timeS *.05 * Speed + Offset;\\n\\n// a raymarching experiment by kabuto\\n//fork by tigrou ind (2013.01.22)\\n\\nvec3 field(vec3 p) {\\n\\tp *= .1;\\n\\tfloat f = .1;\\n\\tfor (int i = 0; i < res; i++) {\\n\\t\\tp = p.yzx*mat3(.6* p1,.6* p1,0,-.6,.6*p1,0,0,0,1);\\n\\t\\tp += vec3(.1*p2,.456* p2,.789*p2)*float(0.1);\\n\\t\\tp = abs(fract(p)-.5);\\n\\t\\tp *= 2.0;\\n\\t\\tf *= 2.001;\\n\\t}\\n\\tp *= p;\\n\\treturn sqrt(p+p.yzx)/f-.035 * Thickness;\\n}\\n\\nvoid main( void ) {\\n\\tfloat jit = 0.01;\\n\\tif (mod(time, 0.1) < 2.0 ) jit = 0.005;\\n\\tvec3 dir = normalize(vec3((gl_FragCoord.xy - resolution * center ) / resolution.x, zoom));\\n\\tfloat a = 0.0;\\n\\tvec3 pos = vec3(x * time, y * time, z * time);\\n\\tdir *= mat3(1,0,0,0,cos(a),-sin(a),0,sin(a),cos(a));\\n\\tdir *= mat3(cos(a),0,-sin(a),0,1,0,sin(a),0,cos(a));\\n\\tvec3 color = vec3(0);\\n\\tfor (int i = 0; i < Itterations; i++) {\\n\\t\\tvec3 f2 = field(pos);\\n\\t\\tfloat f = min(min(f2.x,f2.y),f2.z);\\n\\t\\t\\n\\t\\tpos += dir*f;\\n\\t\\tcolor += float(Itterations-i)/(f2+jit);\\n\\t}\\n\\tvec3 color3 = vec3(1.-1./(1.+color*(.09/float(Itterations*Itterations))));\\n\\tcolor3 *= color3;\\n\\tfragColorOut = vec4(vec3(color3.r+color3.g+color3.b)*tint,1.);\\n}",


"gl33_vert_error": "compiled successfully",


